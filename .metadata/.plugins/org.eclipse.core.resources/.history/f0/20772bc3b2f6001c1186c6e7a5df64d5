package chap02;

public class MaxNum {

	public static void main(String[] args) {
		int[] array = {5, 2, 14, 20, 8, 31, 45, 61};
		int maxNum = 0;
		
		for(int i = 0; i < array.length; i++)
		{
			if(maxNum < array[i])
				maxNum = array[i];
		}
		
		System.out.println("MaxNum: " + maxNum);
		
	}

}
